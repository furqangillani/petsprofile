<div class="container-fluid mt-5">
  <div class="row">
    <div class="col-md-4">
      <div class="card user-card">
        <div class="card-header">
          <h5>Add New Pet</h5>
          <p style="color: red"><%= alert %></p>
        </div>
        <div class="card-block">
          <%= render "form", pet: @pet %>
        </div>
      </div>
    </div>
    <% @pets.each do |pet| %>
      <%= render pet %>
    <% end %>
  </div>
</div>
