<div class="container-fluid mt-5">
  <div class="row">
    <div class="col-md-4">
      <div class="card user-card">
        <div class="card-header">
          <h5>Add New Category</h5>
          <p style="color: red"><%= alert %></p>
        </div>
        <div class="card-block">
          <%= render "form", type: @type %>
        </div>
      </div>
    </div>
    <% @types.each do |type| %>
      <%= render type %>
    <% end %>
  </div>
</div>
